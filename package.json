{
	"name": "cockpitcap",
	"version": "1.0.0",
	"description": "Generated by cds-services-archetype",
	"license": "ISC",
	"repository": "<Add your repository here>",
	"scripts": {
		"setup": "npm install && npm run build && npm run deploy",
		"build": "cds build/all --clean",
		"build:deploy:cf": "npm run build:cf && npm run deploy:cf",
		"build:cf": "mbt build -p cf",
		"schema": "cds compile srv --to sql > srv/src/main/resources/schema.sql",
		"deploy": "cds deploy",
		"deploy:cf": "cf deploy mta_archives/cockpitcap_1.0.0.mtar",
		"start": "mvn spring-boot:run",
		"start:cloud": "mvn spring-boot:run -Dspring-boot.run.profiles=cloud",
		"startv2": "node srv/index"
	},
	"dependencies": {
		"@sap/cds": "^3.33.1",
		"@sap/cds-odata-v2-adapter-proxy": "^1.4.25",
		"express": "^4.17.1",
		"@sap/xsenv": "^2.2.0",
		"hdb": "^0.17.1"
	},
	"cds": {
		"build": {
			"target": "."
		},
		"requires": {
			"db": {
				"kind": "sqlite",
				"model": [
					"db",
					"srv"
				],
				"credentials": {
					"database": "sqlite.db"
				},
				"[production]": {
					"kind": "hana"
				}
			}
		},
		"odata": {
			"version": "v4"
		}
	},
	"devDependencies": {
		"@sap/hdi-deploy": "3.7.0",
		"sqlite3": "^4.1.1"
	}
}
