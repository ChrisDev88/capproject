{
  "welcomeFile": "/app/fiori.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "/v2/(.*)$",
      "target": "/v2/$1",
      "destination": "srv_api_v2"
    },
    {
      "source": "/api/(.*)$",
      "target": "/api/$1",
      "destination": "srv_api"
    },
    {
      "source": "^/app/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}
